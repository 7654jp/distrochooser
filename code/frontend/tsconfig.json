{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "include": [
      "./**/*"
  ],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": false,
    "allowJs": true,
    "resolveJsonModule": true,
    "types": [
      "node"
    ],
    "paths": {
      "~/sdk": [
        "./code/frontend/sdk"
      ],
      "~/states": [
        "./states"
      ]
    }
  }
}
